<div class="text-center">
<h1>部屋情報編集</h1>
<table class="table caption-top">
  <thead>
    <tr class="text-center">
      <th scope="col">Floor</th>
      <th scope="col"></th>
    </tr>
  </thead>

  <tbody>
    <% floor_num=['4','5','6','7','8','9','10','11','12'] %>
    <% floor_num.each do |num| %>
      <tr class="text-center">
      
        <td><%= num %></td>
        <td><%= link_to("詳細","/user/admin/rooms_edit/#{num}") %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<button type="submit" class="btn btn-light mt-4"><%= link_to("部屋情報をすべてリセット","/user/admin/room/reset") %></button>

</div>